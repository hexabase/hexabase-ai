# Hexabase KaaS Docker Compose Environment Variables
# Copy this file to .env and adjust ports if needed

# PostgreSQL
POSTGRES_HOST_PORT=5433
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=hexabase

# Redis
REDIS_HOST_PORT=6380

# NATS
NATS_HOST_PORT=4223
NATS_MONITOR_PORT=8223

# API
API_HOST_PORT=8080

# Development JWT Secret (change in production!)
JWT_SECRET=dev-jwt-secret-change-in-production